<button style="float: left"><a href="/add-product">Add Product</a></button><br><br>
<table border="1" cellpadding="7" cellspacing="7">
  <tr>
    <th>No.</th>
    <th>Product Name:</th>
    <th>Quantity</th>
    <th>Sku</th>
    <th>Description</th>
    <th>Price</th>
    <th>Category</th>
    <th>Photo</th>
    <th>Delete</th>
    <th>Edit</th>
  </tr>
  <% if (data.length) {
        for (let index = 0; index < data.length; index++) { %>
  <tr>
    <td><%= (index+1)%></td>
    <td><a href="/product-detail/<%=data[index].Pid%>"><%= data[index].Pname %></a></td>
    <td><%= data[index].qty %></td>
    <td><%= data[index].sku %></td>
    <td><%= data[index].description %></td>
    <td><%= data[index].price %></td>
    <td><%= data[index].category %></td>
    <td><img src="/../uploads/<%= data[index].photo%>" width=100px height=100px></td>
    <td><a href="/delete/<%= data[index].Pid %>">Delete</a></td>
    <td><a href="/edit/<%=data[index].Pid%>">Edit</a></td>
  </tr>
  <% } 
 
  } else { 

  } %>
</table>
